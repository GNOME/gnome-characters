<schemalist gettext-domain="@PACKAGE_NAME@">
  <schema id="@PACKAGE_NAME@" path="@PACKAGE_DBUS_PATH@/">
    <key name="show-exclamation-mark" type="b">
      <default>true</default>
      <summary>Show exclamation mark</summary>
      <description>
        Use the exclamation mark at the end of "Hello world".
      </description>
    </key>
  </schema>
</schemalist>
